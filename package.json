{
  "name": "wee-promise",
  "version": "0.3.0",
  "description": "an ultra light promises implementation",
  "main": "dist/wee-promise.js",
  "config": {
    "src": [
      "index.js"
    ],
    "lib": [
      "bower_components/briskit/dist/briskit.js"
    ],
    "banner": "/*! <%= pkg.name %> - <%= pkg.version %> - <%= pkg.author.name %> - <%= grunt.config.data.gitinfo.local.branch.current.shortSHA %> - <%= grunt.template.today('yyyy-mm-dd') %> */\n",
    "connect": {
      "port": 9001,
      "base": ".",
      "hostname": "0.0.0.0",
      "url": "http://<%= pkg.config.connect.hostname %>:<%= pkg.config.connect.port %>"
    }
  },
  "devDependencies": {
    "fs-extra": "~0.9.1",
    "es6-promise": ">=2.0.1",
    "colors": "*",
    "grunt": ">=0.4.2",
    "grunt-contrib-jshint": ">=0.10.0",
    "grunt-contrib-concat": ">=0.5.1",
    "grunt-contrib-uglify": ">=0.5.0",
    "grunt-contrib-clean": ">=0.4.1",
    "grunt-gitinfo": ">=0.1.7",
    "grunt-contrib-watch": ">=0.6.1",
    "grunt-replace": ">=0.7.7",
    "grunt-contrib-connect": ">=0.11.1",
    "grunt-mocha-phantomjs": ">=1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elnarddogg/wee-promise.git"
  },
  "keywords": [
    "promise"
  ],
  "author": {
    "name": "Bernard McManus",
    "email": "bernard.mcmanus.iv@gmail.com",
    "url": "http://github.com/elnarddogg"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/elnarddogg/wee-promise/blob/master/LICENSE.txt"
  },
  "bugs": {
    "url": "https://github.com/elnarddogg/wee-promise/issues"
  },
  "homepage": "https://github.com/elnarddogg/wee-promise"
}
